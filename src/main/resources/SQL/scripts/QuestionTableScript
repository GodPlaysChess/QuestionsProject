CREATE  TABLE `questionsproject`.`Questions` (
  `id` INT NOT NULL AUTO_INCREMENT ,
  `type` VARCHAR(45) NOT NULL ,
  `text` VARCHAR(45) NOT NULL ,
  PRIMARY KEY (`id`) );


